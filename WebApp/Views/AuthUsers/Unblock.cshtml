@model WebApp.Areas.Identity.Data.Users
@{
    ViewData["Title"] = "Unblock";
}
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FirstName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LastLogin)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RegTime)
        </dt>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IsActive)
        </dt>
    </dl>

    <form asp-action="Unblock">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Unblock" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>